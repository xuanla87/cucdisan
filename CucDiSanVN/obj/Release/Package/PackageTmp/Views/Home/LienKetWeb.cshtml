@model IEnumerable<CucDiSanService.Models.LienKetWeb>
<div class="box">
    <div class="box-title">
        Liên kết website
    </div>
    <div class="lien-ket">
        <select class="form-control" id="lienket" onselect="chuyenlink()">
            <option>-- Liên kết website --</option>
            @if (Model != null && Model.Count() > 0)
            {
                foreach (var item in Model)
                {
                    <option value="@item.lienKetLink">@Html.Raw(item.lienKetName)</option>
                }
            }
        </select>
    </div>
</div>
